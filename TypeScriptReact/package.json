{
  "name": "iobroker.template",
  "version": "0.0.1",
  "description": "Template for adapter development",
  "author": {
    "name": "Author",
    "email": "author@mail.com"
  },
  "homepage": "https://github.com/Author/ioBroker.template",
  "license": "MIT",
  "keywords": [
    "ioBroker",
    "template",
    "Smart Home",
    "home automation"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Author/ioBroker.template"
  },
  "dependencies": {
    "@iobroker/adapter-core": "^2.4.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.14",
    "@babel/core": "^7.13.14",
    "@babel/plugin-proposal-decorators": "^7.13.5",
    "@babel/preset-env": "^7.13.12",
    "@babel/preset-typescript": "^7.13.0",
    "@iobroker/adapter-react": "^1.6.8",
    "@iobroker/testing": "^2.4.4",
    "@material-ui/core": "^4.11.3",
    "@material-ui/icons": "^4.11.2",
    "@types/chai": "^4.2.16",
    "@types/chai-as-promised": "^7.1.3",
    "@types/gulp": "^4.0.8",
    "@types/mocha": "^8.2.2",
    "@types/node": "^14.14.37",
    "@types/proxyquire": "^1.3.28",
    "@types/react-dom": "^16.9.12",
    "@types/react": "^16.14.5",
    "@types/sinon": "^9.0.11",
    "@types/sinon-chai": "^3.2.5",
    "@typescript-eslint/eslint-plugin": "^4.21.0",
    "@typescript-eslint/parser": "^4.21.0",
    "axios": "^0.21.1",
    "chai": "^4.3.4",
    "chai-as-promised": "^7.1.1",
    "eslint": "^7.23.0",
    "eslint-plugin-react": "^7.23.1",
    "gulp": "^4.0.2",
    "mocha": "^8.3.2",
    "nyc": "^15.1.0",
    "parcel-bundler": "^1.12.5",
    "proxyquire": "^2.1.3",
    "react-dom": "^16.14.0",
    "react-icons": "^4.2.0",
    "react": "^16.14.0",
    "rimraf": "^3.0.2",
    "sinon": "^10.0.0",
    "sinon-chai": "^3.6.0",
    "source-map-support": "^0.5.19",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3"
  },
  "main": "build/main.js",
  "scripts": {
    "prebuild": "rimraf ./build",
    "build:parcel": "parcel build admin/src/index.tsx admin/src/tab.tsx -d admin/build",
    "build:ts": "tsc -p tsconfig.build.json",
    "build": "npm run build:ts && npm run build:parcel",
    "watch:parcel": "parcel admin/src/index.tsx admin/src/tab.tsx -d admin/build",
    "watch:ts": "tsc -p tsconfig.build.json --watch",
    "watch": "npm run watch:ts",
    "test:ts": "mocha --config test/mocharc.custom.json src/**/*.test.ts",
    "test:package": "mocha test/package --exit",
    "test:unit": "mocha test/unit --exit",
    "test:integration": "mocha test/integration --exit",
    "test": "npm run test:ts && npm run test:package",
    "check": "tsc --noEmit",
    "coverage": "nyc npm run test:ts",
    "lint": "eslint --ext .ts,.tsx src/ admin/src/"
  },
  "nyc": {
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "src/**/*.test.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text-summary",
      "html"
    ],
    "sourceMap": true,
    "instrument": true
  },
  "bugs": {
    "url": "https://github.com/Author/ioBroker.template/issues"
  },
  "readmeFilename": "README.md"
}